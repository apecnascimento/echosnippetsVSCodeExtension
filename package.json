{
    "name": "echosnippets",
    "displayName": "EchoSnippets",
    "description": "Go Echo framework web server snippets",
    "version": "1.0.0",
    "publisher": "Makhmud",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/makhmudislamov/echosnippetsVSCodeExtension"
    },
    "engines": {
        "vscode": "^1.31.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "go",
                "path": "./snippets/hellosnippet.json"
